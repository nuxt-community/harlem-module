{
  "name": "@nuxtjs/harlem",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Harlem integration for Nuxt",
  "repository": "nuxt-community/harlem-module",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "harlem",
    "store"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "prepack": "yarn build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "test:e2e": "vitest run test/e2e"
  },
  "dependencies": {
    "@harlem/core": "^2.3.1",
    "@harlem/plugin-devtools": "^2.3.1",
    "@harlem/plugin-ssr": "^2.3.1",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "knitwork": "^0.1.1",
    "scule": "^0.2.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.1.7",
    "@nuxt/test-utils": "npm:@nuxt/test-utils-edge@latest",
    "@nuxtjs/eslint-config-typescript": "9.0.0",
    "@release-it/conventional-changelog": "4.2.2",
    "@types/serve-static": "1.13.10",
    "c8": "^7.11.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "nuxt3": "^3.0.0-27467464.a0c4d6e",
    "playwright": "^1.20.0",
    "prettier": "2.6.0",
    "release-it": "14.13.1",
    "vite": "^2.8.6",
    "vitest": "^0.7.10"
  }
}
